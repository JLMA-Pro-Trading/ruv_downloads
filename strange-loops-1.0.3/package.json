{
  "name": "strange-loops",
  "version": "1.0.3",
  "description": "Emergent intelligence through temporal consciousness - thousands of nano-agents collaborating in real-time with 500K+ ops/sec",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "strange-loops": "dist/cli/index.js",
    "strange-loop": "dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./mcp": {
      "import": "./dist/mcp/index.js",
      "types": "./dist/mcp/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist/",
    "test": "node --test tests/",
    "start": "npm run build && node dist/cli/index.js",
    "mcp": "npm run build && node dist/cli/index.js mcp start",
    "demo": "npm run build && node dist/cli/index.js demo",
    "benchmark": "npm run build && node dist/cli/index.js benchmark"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.1",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "ora": "^5.4.1",
    "sublinear-time-solver": "^1.4.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "strange-loops",
    "consciousness",
    "temporal-computing",
    "quantum-classical",
    "nano-agents",
    "emergent-intelligence",
    "mcp",
    "model-context-protocol",
    "swarm-intelligence",
    "real-time",
    "nanosecond",
    "distributed-computing"
  ],
  "author": "rUv <github.com/ruvnet>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruvnet/strange-loops.git"
  },
  "bugs": {
    "url": "https://github.com/ruvnet/strange-loops/issues"
  },
  "homepage": "https://github.com/ruvnet/strange-loops#readme"
}
