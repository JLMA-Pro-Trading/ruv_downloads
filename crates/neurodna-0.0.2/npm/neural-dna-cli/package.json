{
  "name": "neural-dna-cli",
  "version": "1.0.0",
  "description": "Neural DNA CLI - Genetic algorithms for neural networks with WASM acceleration",
  "main": "lib/index.js",
  "bin": {
    "neural-dna": "./bin/neural-dna.js"
  },
  "scripts": {
    "build": "npm run build:js",
    "build:wasm": "echo 'WASM build disabled - using JavaScript fallback'",
    "build:js": "tsc",
    "test": "jest",
    "test:docker": "docker run --rm -v $(pwd):/app -w /app node:18-alpine npm test",
    "test:local": "npm test && npm run test:integration",
    "test:integration": "node test/integration.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build:js",
    "start": "node bin/neural-dna.js",
    "dev": "npm run build && npm start"
  },
  "keywords": [
    "neural-networks",
    "genetic-algorithms",
    "ai",
    "machine-learning",
    "cli",
    "wasm",
    "rust",
    "evolution",
    "optimization"
  ],
  "author": "rUv Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/ruv-FANN.git",
    "directory": "npm/neural-dna-cli"
  },
  "bugs": {
    "url": "https://github.com/ruvnet/ruv-FANN/issues"
  },
  "homepage": "https://github.com/ruvnet/ruv-FANN#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "bin/",
    "lib/",
    "wasm/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.3.8",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.0.0",
    "wasm-pack": "^0.12.0"
  },
  "preferGlobal": true
}
