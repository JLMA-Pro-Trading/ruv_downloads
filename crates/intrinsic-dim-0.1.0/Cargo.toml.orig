[package]
name = "intrinsic-dim"
version = "0.1.0"
edition = "2021"
authors = ["ruvnet"]
description = "Fast intrinsic dimensionality estimation for ML model optimization"
documentation = "https://docs.rs/intrinsic-dim"
homepage = "https://github.com/ruvnet/intrinsic-dim"
repository = "https://github.com/ruvnet/intrinsic-dim"
readme = "README.md"
license = "MIT"
keywords = ["dimensionality", "compression", "ml", "optimization", "emergence"]
categories = ["algorithms", "science", "mathematics", "compression"]

[dependencies]
ndarray = "0.15"
ndarray-rand = "0.14"
rand = "0.8"
rand_distr = "0.4"
rayon = { version = "1.10", optional = true }
serde = { version = "1.0", features = ["derive"] }
serde_json = "1.0"
approx = "0.5"
num-traits = "0.2"

[dev-dependencies]
criterion = "0.5"
proptest = "1.0"

[features]
default = []
parallel = ["rayon"]

[[bench]]
name = "intrinsic_bench"
harness = false

[[example]]
name = "basic_usage"

[[example]]
name = "fourier_features"

[[example]]
name = "image_compression"
